{
  "name": "sylvaindethier.github.io",
  "private": true,
  "version": "0.1.0",
  "description": "Sylvain Dethier personal website",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sylvaindethier/sylvaindethier.github.io.git"
  },
  "author": "Sylvain Dethier <dethiersylvain@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sylvaindethier/sylvaindethier.github.io/issues"
  },
  "homepage": "https://sylvaindethier.github.io",
  "scripts": {
    "install:submodule": "git submodule init && git submodule update",
    "install:cra": "cd create-react-app && npm install && cd ..",
    "install:flow-typed": "rimraf ./flow-typed/npm && flow-typed install",
    "postinstall": "npm run install:flow-typed",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "build": "node scripts/build.js",
    "serve": "node scripts/serve.js",
    "deploy": "node scripts/deploy.js"
  },
  "dependencies": {
    "material-ui": "^0.19.4",
    "object-assign": "^4.1.1",
    "promise": "7.1.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-icons": "^2.2.3",
    "react-loadable": "^3.2.2",
    "react-router-dom": "^4.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "sanitize.css": "^5.0.0",
    "whatwg-fetch": "^2.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.0.0-beta.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-app": "file:./create-react-app/packages/babel-preset-react-app",
    "case-sensitive-paths-webpack-plugin": "^2.0.0",
    "chalk": "^2.3.0",
    "connect-history-api-fallback": "^1.3.0",
    "cross-spawn": "^5.1.0",
    "css-loader": "^0.28.0",
    "detect-port": "^1.0.7",
    "dotenv": "^4.0.0",
    "empty": "^0.10.1",
    "eslint": "^4.10.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-config-react-app": "file:./create-react-app/packages/eslint-config-react-app",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-flowtype": "^2.30.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.59.0",
    "flow-typed": "^2.0.0",
    "fs-extra": "^4.0.2",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.4",
    "jest": "^21.2.1",
    "postcss-loader": "^1.3.3",
    "prettier": "^1.8.2",
    "react-dev-utils": "file:./create-react-app/packages/react-dev-utils",
    "rimraf": "^2.6.1",
    "serve": "^6.4.1",
    "style-loader": "^0.16.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-manifest-plugin": "^1.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app",
      "flow",
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "syntax-dynamic-import",
      [
        "transform-class-properties",
        {
          "spec": true
        }
      ],
      [
        "transform-es2015-modules-commonjs",
        {
          "spec": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier",
      "prettier/flowtype",
      "prettier/react"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {}
      ]
    }
  }
}
