{
  "presets": [
    "es2015",
    "es2016",
    "es2017",
    "react",
  ],

  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true
    }]
  ],

  "env": {
    "production": {
      "plugins": [
        "transform-react-constant-elements",
        ["react-intl", {
          "messagesDir": "./build/messagesIntl/",
          "enforceDescriptions": true
        }]
      ]
    }
  }
}
